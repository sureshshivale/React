<script>
   $(function () { 
    $('#myModal').modal({});
     var winH = $(window).height();
	 var winW = $(window).width();

	// Set the popup window to center
	$('#modd').css('top', winH / 3.5 - $('#modd').height()/2);
	$('#modd').css('left', (winW / 2 - $('#modd').width())-($('#modd').width()/7));
	 
   
   });
